<%= simple_form_for @song, html: {multipart: true} do |f| %>
	<%= f.association :composer %>
	<%= f.input :title %>
	<%= f.input :description, hint: 'Brief description of what the song is about' %>
	<%= f.input :medialink, as: :file, hint: 'upload Media files here' %>
	<%= f.input :lyrics %>
	<%= f.association :place %>
	<%= f.button :submit %>
<% end %>
