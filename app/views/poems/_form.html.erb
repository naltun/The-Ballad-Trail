<%= simple_form_for @poem, html: {multipart: true} do |f| %>
	<%= f.input :title %>
	<%= f.input :description, hint: 'Brief description of what the poem is about' %>
	<%= f.input :medialink, as: :file %>
	<%= f.input :subtitle %>
	<%= f.association :place %>
	<%= f.association :composer %>
	<%= f.button :submit %>
<% end %>